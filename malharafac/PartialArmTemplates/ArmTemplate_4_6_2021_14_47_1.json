							"name": "AlterRow1"
						}
					],
					"script": "source(output(\n\t\tProductID as integer,\n\t\tName as string,\n\t\tProductNumber as string,\n\t\tColor as string,\n\t\tStandardCost as decimal(19,4),\n\t\tListPrice as decimal(19,4),\n\t\tSize as string,\n\t\tWeight as decimal(8,2),\n\t\tProductCategoryID as integer,\n\t\tProductModelID as integer,\n\t\tSellStartDate as timestamp,\n\t\tSellEndDate as timestamp,\n\t\tDiscontinuedDate as timestamp,\n\t\tThumbNailPhoto as binary,\n\t\tThumbnailPhotoFileName as string,\n\t\trowguid as string,\n\t\tModifiedDate as timestamp\n\t),\n\tallowSchemaDrift: true,\n\tvalidateSchema: false,\n\tisolationLevel: 'READ_UNCOMMITTED',\n\tformat: 'table') ~> salesproduct\nsource(output(\n\t\tProductId as integer,\n\t\tName as string,\n\t\tcolor as string,\n\t\tstandardcost as integer,\n\t\tlistprise as integer,\n\t\tproductcategoryid as integer,\n\t\thashkey as string,\n\t\tinserteddate as timestamp,\n\t\tupdateddate as timestamp\n\t),\n\tallowSchemaDrift: true,\n\tvalidateSchema: false,\n\tisolationLevel: 'READ_UNCOMMITTED',\n\tformat: 'table') ~> dimproduct\nsalesproduct derive(newHashKey = sha1(Name+ProductNumber+Color)) ~> DerivedColumn1\nDerivedColumn1, dimproduct exists(newHashKey == hashkey,\n\tnegate:true,\n\tbroadcast: 'auto')~> Exists1\nExists1, dimproduct lookup(salesproduct@ProductID == dimproduct@ProductId,\n\tmultiple: false,\n\tpickup: 'any',\n\tbroadcast: 'auto')~> Lookup1\nLookup1 derive(newInsertedDate = iif(isNull(inserteddate) , currentTimestamp() , inserteddate  ),\n\t\tnewUpdatedDate = currentTimestamp()) ~> DerivedColumn2\nDerivedColumn2 alterRow(upsertIf(true())) ~> AlterRow1\nAlterRow1 sink(input(\n\t\tProductId as integer,\n\t\tName as string,\n\t\tcolor as string,\n\t\tstandardcost as integer,\n\t\tlistprise as integer,\n\t\tproductcategoryid as integer,\n\t\thashkey as string,\n\t\tinserteddate as timestamp,\n\t\tupdateddate as timestamp\n\t),\n\tallowSchemaDrift: true,\n\tvalidateSchema: false,\n\tdeletable:false,\n\tinsertable:false,\n\tupdateable:false,\n\tupsertable:true,\n\tkeys:['ProductId'],\n\tformat: 'table',\n\tmapColumn(\n\t\tProductId = salesproduct@ProductID,\n\t\tName = salesproduct@Name,\n\t\tcolor = salesproduct@Color,\n\t\tstandardcost = salesproduct@StandardCost,\n\t\tlistprise,\n\t\tproductcategoryid = salesproduct@ProductCategoryID,\n\t\thashkey = newHashKey,\n\t\tinserteddate = newInsertedDate,\n\t\tupdateddate = newUpdatedDate\n\t),\n\tskipDuplicateMapInputs: true,\n\tskipDuplicateMapOutputs: true,\n\terrorHandlingOption: 'stopOnFirstError') ~> sink1"
