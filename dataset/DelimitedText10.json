{
	"name": "DelimitedText10",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"year": {
				"type": "string"
			},
			"month": {
				"type": "string"
			},
			"dy": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().dy",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().year,'\\',dataset().month)",
					"type": "Expression"
				},
				"container": "salesdata"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}